    "enzyme": "^3.1.0",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.0.0",
